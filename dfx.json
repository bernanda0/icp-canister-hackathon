{
  "canisters": {
    "testing": {
      "main": "src/motomoto_backend/main.mo",
      "type": "motoko"
    },
    "pub": {
      "type": "motoko",
      "main": "src/motomoto_backend/pub/main.mo"
    },
    "sub": {
      "type": "motoko",
      "main": "src/motomoto_backend/sub/main.mo"
    },
    "motomoto_frontend": {
      "dependencies": [
        "testing",
        "pub",
        "sub"
      ],
      "frontend": {
        "entrypoint": "src/omnia_main_frontend/build/index.html"
      },
      "source": [
        "src/motomoto_frontend/build"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}